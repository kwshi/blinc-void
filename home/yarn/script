# vi: ft=bash
# shellcheck shell=bash

local -a pkgs
readarray -t pkgs < <(normalize_list <<< '

typescript
ttypescript

prettier
eslint
typescript-language-server

@elm-tooling/elm-language-server
elm-format
elm-test

svelte-language-server

netlify-cli
vercel

')

buildah run "$1" yarn global add "${pkgs[@]}"
